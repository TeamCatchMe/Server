{
  "name": "catchme_server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && tsc-alias",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start-server:dev": "cross-env NODE_ENV=development node dist/main.js",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "db-pull": "dotenv -e .env.development -- prisma db pull && prisma generate --schema ./prisma/schema.prisma",
    "db-push": "dotenv -e .env.development -- prisma db push && prisma generate --schema ./prisma/schema.prisma",
    "db-pull:prod": "cross-env NODE_ENV=production npx prisma db pull && prisma generate --schema ./prisma/schema.prisma",
    "db-push:prod": "cross-env NODE_ENV=production npx prisma db push && prisma generate --schema ./prisma/schema.prisma",
    "generate": "prisma generate --schema ./prisma/schema.prisma",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/terminus": "^9.0.0",
    "@prisma/client": "^4.0.0",
    "@slack/client": "^5.0.2",
    "aws-sdk": "^2.1167.0",
    "browserless": "^9.5.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^6.4.0",
    "global": "^4.4.0",
    "helmet": "^5.1.0",
    "lodash": "^4.17.21",
    "multer-s3": "^2.10.0",
    "multer-s3-transform": "^2.10.3",
    "nanoid": "^3.3.4",
    "nest-raven": "^8.2.0",
    "nest-winston": "^1.6.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "prisma": "^4.1.0",
    "random": "^3.0.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.30.7",
    "swagger-ui-express": "^4.4.0",
    "user-agents": "^1.0.1131",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/testing": "^8.0.0",
    "@types/cache-manager": "^4.0.1",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.12",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/sharp": "^0.30.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fast-shuffle": "^5.0.1",
    "husky": "^8.0.0",
    "jest": "28.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.0.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "^@modules/v1/(.*)$": "<rootDir>/src/modules/v1/$1",
      "^@common/(.*)$": "<rootDir>/src/common/$1",
      "^@helper/(.*)$": "<rootDir>/src/helper/$1",
      "^@config/(.*)$": "<rootDir>/src/config/$1"
    }
  }
}
